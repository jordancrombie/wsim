    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "expo-server-sdk": "^4.0.0",
    "express": "^5.2.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
